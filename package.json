{
  "name": "mni",
  "version": "0.0.1-alpha",
  "description": "Mnimal frontend framework",
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "bourbon": "^6.0.0",
    "chokidar-cli": "^2.0.0",
    "clean-css-loader": "^2.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "fibers": "^4.0.1",
    "imports-loader": "^0.8.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mqpacker": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "sass": "^1.23.0",
    "sass-loader": "^8.0.0",
    "sassdoc": "^2.7.1",
    "terser-webpack-plugin": "^2.2.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-merge": "^4.2.2",
    "webpack-modernizr-loader": "^5.0.0"
  },
  "author": {
    "name": "Marc Eickhoff",
    "email": "mail@marceickhoff.com",
    "url": "https://marceickhoff.com/"
  },
  "repository": "github:marceickhoff/mni",
  "bugs": {
    "url": "https://github.com/marceickhoff/mni/issues"
  },
  "license": "GPL-3.0",
  "scripts": {
    "build:prod": "cross-env NODE_ENV=production webpack --minimal",
    "build:dev": "cross-env NODE_ENV=development webpack --minimal",
    "build": "run-p build:*",
    "watch:prod": "chokidar \"lib/**/*\" -c \"npm run build:prod\",",
    "watch:dev": "chokidar \"lib/**/*\" -c \"npm run build:dev\",",
    "watch": "chokidar \"lib/**/*\" -c \"npm run build\",",
    "docs": "sassdoc lib/scss -d docs",
    "postinstall": "npm run build",
    "prepublish": "npm run build & npm run docs"
  },
  "files": [
    "dist/css/main.min.css",
    "dist/js/main.min.js"
  ],
  "dependencies": {
    "reset-css": "^4.0.1",
    "vanilla-lazyload": "^12.3.0",
    "zooming": "^2.1.1"
  }
}
